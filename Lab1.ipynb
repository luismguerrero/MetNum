    "import math\n",
    "g=9.8\n",
    "mas=68.1\n",
    "c=12.5\n",
    "t=0\n",
    "\n",
    "def vt (t):\n",
    "    return (g*mas/c)*(1-math.exp(-(c/mas)*(t)))\n",
    "while (t<40):\n",
    "    print t, \" \", vt(t)\n",
    "    t+=1\n",
    "    "
